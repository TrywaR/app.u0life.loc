<?xml version='1.0' encoding='utf-8'?>
<widget id="u0life.com" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>u0life</name>
    <description>
        TrywaR [dev]
    </description>
    <author email="send@trywar.ru" href="http://cordova.io">
        TrywaR [dev]
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="StatusBarOverlaysWebView" value="false" />
    <preference name="StatusBarBackgroundColor" value="#7661db" />
    <preference name="StatusBarStyle" value="blacktranslucent" />
    <preference name="SplashShowOnlyFirstTime" value="true" />
    <preference name="SplashScreenDelay" value="3000" />
    <preference name="AutoHideSplashScreen" value="true" />
    <preference name="SplashMaintainAspectRatio" value="true" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <platform name="browser">
        <preference name="ShowSplashScreen" value="false" />
        <gcm_sender_id value="131713096606" />
    </platform>
</widget>
